{
  "name": "uma-saas-fe",
  "version": "1.0.0",
  "description": "优码前端主项目",
  "main": "index.js",
  "scripts": {
    "mock": "esky-mock -c mock/config.js",
    "start-mock": "concurrently -k \"npm run mock\" \"MOCK=true npm run start\"",
    "start": "npm run start-qa",
    "start-qa": "env-cmd -e dev webpack serve --config ./config/webpack.dev.js",
    "start-online": "env-cmd -e devOnline webpack serve --config ./config/webpack.dev.js",
    "build": "env-cmd -e online webpack --config ./config/webpack.dev.js",
    "build-dev": "env-cmd -e dev webpack --config ./config/webpack.prod.js",
    "build-qa": "env-cmd -e qa webpack --config ./config/webpack.prod.js",
    "build-online": "env-cmd -e online webpack --config ./config/webpack.prod.js",
    "build-dll": "env-cmd -e online webpack --config ./config/webpack.dll.js",
    "analyzer": "env-cmd -e online,analysis webpack --config ./config/webpack.prod.js"
  },
  "author": "",
  "license": "ISC",
  "prettier": "@tencent/tcf-prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "tcf-commit -t msg"
    }
  },
  "lint-staged": {
    "*.{html,wxml,md,json}": [
      "prettier --write"
    ],
    "*.{css,less,scss,wxss}": [
      "stylelint --fix --allow-empty-input"
    ],
    "*.{js?(x),ts?(x)}": [
      "eslint --fix"
    ]
  },
